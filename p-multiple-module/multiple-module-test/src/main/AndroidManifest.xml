<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hshare.multiple.module.test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HsharePfActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.jaeger.statusbarutil.MainActivity" android:theme="@style/AppTheme1"/>
        <activity android:name="com.jaeger.statusbarutil.ColorStatusBarActivity" android:theme="@style/AppTheme1"/>
        <activity android:name="com.jaeger.statusbarutil.ImageStatusBarActivity" android:theme="@style/AppTheme1"/>
        <activity android:name="com.jaeger.statusbarutil.ImageViewActivity" android:theme="@style/AppTheme1"/>
        <activity android:name="com.jaeger.statusbarutil.UseInFragmentActivity" android:theme="@style/AppTheme1"/>
        <activity android:name="com.jaeger.statusbarutil.SwipeBackActivity" android:theme="@style/AppTheme1"/>
        <activity android:name="com.jaeger.statusbarutil.SwitchModeActivity" android:theme="@style/AppTheme1"/>
    </application>

</manifest>